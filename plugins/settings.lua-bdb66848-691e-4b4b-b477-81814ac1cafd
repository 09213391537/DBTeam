--By @MaSkAoS & @Josepdal

do

local function run(msg)
    local hash = 'stickers:'..msg.to.id
    if redis:get(hash) then
        sStickers = 'Enabled'
        sStickersD = 'ðŸ”¸'
    else
        sStickers = 'Disabled'
        sStickersD = 'ðŸ”¹'
    end
    local hash = 'anti-flood:enabled:'..msg.to.id
    if redis:get(hash) then
        sFlood = 'Enabled'
        sFloodD = 'ðŸ”¸'
    else
        sFlood = 'Disabled'
        sFloodD = 'ðŸ”¹'
    end
    local hash = 'antilink:enabled:'..msg.to.id
    if redis:get(hash) then
        sLink = 'Enabled'
        sLinkD = 'ðŸ”¸'
    else
        sLink = 'Disabled'
        sLinkD = 'ðŸ”¹'
    end
    local hash = 'antiarabe:enabled:'..msg.to.id
    if redis:get(hash) then
        sArabe = 'Enabled'
        sArabeD = 'ðŸ”¸'
    else
        sArabe = 'Disabled'
        sArabeD = 'ðŸ”¹'
    end
    local hash = 'antibot:enabled:'..msg.to.id
    if redis:get(hash) then
        sBots = 'Enabled'
        sBotsD = 'ðŸ”¸'
    else
        sBots = 'Disabled'
        sBotsD = 'ðŸ”¹'
    end
    local hash = 'name:enabled:'..msg.to.id
    if redis:get(hash) then
        sName = 'Enabled'
        sNameD = 'ðŸ”¸'
    else
        sName = 'Disabled'
        sNameD = 'ðŸ”¹'
    end
    local hash = 'gifs:enabled:'..msg.to.id
    if redis:get(hash) then
        sGif = 'Enabled'
        sGifD = 'ðŸ”¸'
    else
        sGif = 'Disabled'
        sGifD = 'ðŸ”¹'
    end
    local hash = 'photo:enabled:'..msg.to.id
    if redis:get(hash) then
        sPhoto = 'Enabled'
        sPhotoD = 'ðŸ”¸'
    else
        sPhoto = 'Disabled'
        sPhotoD = 'ðŸ”¹'
    end
    text = 'âš™ Group settings:\n'
    text = text..sStickersD..' Anti-Stickers: '..sStickers..'\n'
    text = text..sFloodD..' Anti-Flood: '..sFlood..'\n'
    text = text..sLinkD..' Anti-Link: '..sLink..'\n'
    text = text..sArabeD..' Anti-Arabic: '..sArabe..'\n'
    text = text..sBotsD..' Anti-Bots: '..sBots..'\n'
    text = text..sGifD..' Anti-Gifs: '..sGif..'\n'
    text = text..sPhotoD..' Anti-Photo: '..sPhoto..'\n'
    text = sNameD..' Lock-Name: '..sName
    if msg.to.type == 'chat' then
        send_msg('chat#id'..msg.to.id, text, ok_cb, false)
    elseif msg.to.type == 'channel' then
        send_msg('channel#id'..msg.to.id, text, ok_cb, false)
    end
end

return {
patterns = {
'^#(settings)$'
},
run = run
}
end